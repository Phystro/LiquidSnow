<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Authors>Thismaker</Authors>
    <Company>Thismaker</Company>
    <Description>Allows the easy configuration of AbaClients using configuration sources like JSON.</Description>
    <PackageIcon>Thisicon-Prime.png</PackageIcon>
    <RepositoryUrl>https://github.com/thismaker/LiquidSnow</RepositoryUrl>
    <PackageTags>client-application client-app signalr webapi web-api client-server</PackageTags>
    <PackageReleaseNotes>First release. Very frozen. Don't fear using.</PackageReleaseNotes>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <RepositoryType>git</RepositoryType>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.Configuration" Version="5.0.0" />
    <PackageReference Include="Microsoft.Extensions.Configuration.Binder" Version="5.0.0" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Client\Thismaker.Aba.Client.csproj" />
    <ProjectReference Include="..\Msal\Thismaker.Aba.Client.Msal.csproj" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\..\..\..\assets\images\Thisicon-Prime.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
